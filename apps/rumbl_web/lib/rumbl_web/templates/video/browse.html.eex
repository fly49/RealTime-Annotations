<h1> Browse Videos</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>
  <tbody>
<%= for video <- @videos do %>
    <tr>
      <td><%= video.title %></td>
      <td>
        <%= link "Watch", to: Routes.watch_path(@conn, :show, video), class: "button" %>
      </td>
    </tr>
<% end %>
  </tbody>
</table>

<span><%= link "Add Video", to: Routes.video_path(@conn, :new) %></span>
